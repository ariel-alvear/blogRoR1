<h1>Blog Principal</h1>
<table class="table">
    <tr>
        <th>Título</th>
        <th>Imagen</th>
        <th>Contenido</th>
        <th>Fecha de creación</th>
    </tr>
    <% @post.each do |post| %>
    <tr>
        <td><%= post.title %> </td>
        <td><img src="<%= post.image_url %>" width="250px" height="250px"> </td>
        <td><%= post.content %> </td>
        <td><%= post.created_at %></td>
    </tr>
    <% end %>
</table>